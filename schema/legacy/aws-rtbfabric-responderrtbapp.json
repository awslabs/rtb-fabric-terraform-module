{
    "typeName": "AWS::RTBFabric::ResponderRtbApp",
    "description": "Resource Type definition for AWS::RTBFabric::ResponderRtbApp Resource Type",
    "definitions": {
        "AppName": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255
        },
        "Description": {
            "type": "string",
            "pattern": "^[A-Za-z0-9 ]+$"
        },
        "VpcId": {
            "type": "string",
            "maxLength": 50,
            "minLength": 5,
            "relationshipRef": {
                "typeName": "AWS::EC2::VPC",
                "propertyPath": "/properties/VpcId"
            }
        },
        "SubnetIds": {
            "type": "array",
            "description": "The ID of one or more subnets in order to create a rtbapp.",
            "uniqueItems": true,
            "insertionOrder": false,
            "items": {
                "type": "string",
                "relationshipRef": {
                    "typeName": "AWS::EC2::Subnet",
                    "propertyPath": "/properties/SubnetId"
                }
            }
        },
        "SecurityGroupIds": {
            "type": "array",
            "description": "The ID of one or more security groups in order to create a rtbapp.",
            "uniqueItems": true,
            "insertionOrder": false,
            "items": {
                "type": "string",
                "anyOf": [
                    {
                        "relationshipRef": {
                            "typeName": "AWS::EC2::SecurityGroup",
                            "propertyPath": "/properties/GroupId"
                        }
                    },
                    {
                        "relationshipRef": {
                            "typeName": "AWS::EC2::SecurityGroup",
                            "propertyPath": "/properties/Id"
                        }
                    }
                ]
            }
        },
        "ClientToken": {
            "type": "string"
        },
        "RtbAppId": {
            "type": "string",
            "pattern": "^rtbapp-[a-z0-9-]{1,25}$"
        },
        "Status": {
            "type": "string",
            "enum": [
                "NEW",
                "ACTIVATING",
                "ACTIVE",
                "DELETING",
                "DELETED",
                "ERROR",
                "UPDATING",
                "ISOLATING",
                "ISOLATED",
                "RESTORING"
            ]
        },
        "CreatedTimestamp": {
            "type": "string"
        },
        "UpdatedTimestamp": {
            "type": "string"
        },
        "Arn": {
            "type": "string",
            "maxLength": 2048,
            "minLength": 20,
            "pattern": "^arn:aws:mpofxdevmu:[a-zA-Z0-9_-]+:[0-9]{12}:rtbApp/[a-zA-Z0-9-]+$"
        },
        "Port": {
            "type": "integer",
            "minimum": 1,
            "maximum": 65535
        },
        "Protocol": {
            "type": "string",
            "enum": [
                "HTTP",
                "HTTPS"
            ]
        },
        "DnsName": {
            "type": "string",
            "maxLength": 255,
            "minLength": 0,
            "pattern": "^(?:[A-Za-z0-9](?:[A-Za-z0-9-]{0,61}[A-Za-z0-9])?)(?:\\.(?:[A-Za-z0-9](?:[A-Za-z0-9-]{0,61}[A-Za-z0-9])?))+$"
        },
        "CaCertificateChain": {
            "type": "string",
            "maxLength": 2097152,
            "minLength": 1
        },
        "TargetGroupList": {
            "type": "array",
            "uniqueItems": true,
            "insertionOrder": false,
            "items": {
                "type": "string",
                "pattern": "^arn:aws:elasticloadbalancing:[a-z0-9-]+:[0-9]{12}:targetgroup\\/[a-zA-Z0-9-_]+\\/[a-f0-9]+$"
            }
        },
        "AutoScalingGroupNameList": {
            "type": "array",
            "uniqueItems": true,
            "insertionOrder": false,
            "items": {
                "type": "string"
            }
        },
        "AutoScalingGroupsConfiguration": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "AutoScalingGroupNameList": {
                    "$ref": "#/definitions/AutoScalingGroupNameList"
                },
                "RoleArn": {
                    "type": "string",
                    "relationshipRef": {
                        "typeName": "AWS::IAM::Role",
                        "propertyPath": "/properties/Arn"
                    }
                }
            },
            "required": [
                "AutoScalingGroupNameList",
                "RoleArn"
            ]
        },
        "TargetGroupsConfiguration": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "TargetGroupArns": {
                    "$ref": "#/definitions/TargetGroupList"
                }
            },
            "required": [
                "TargetGroupArns"
            ]
        },
        "EksEndpointsConfiguration": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "EndpointsResourceName": {
                    "type": "string",
                    "pattern": "^[A-Za-z0-9][A-Za-z0-9-]{0,61}[A-Za-z0-9]$"
                },
                "EndpointsResourceNamespace": {
                    "type": "string",
                    "pattern": "^[A-Za-z0-9][A-Za-z0-9-]{0,61}[A-Za-z0-9]$"
                },
                "ClusterApiServerEndpointUri": {
                    "type": "string",
                    "pattern": "^(https|http)://(?:[A-Za-z0-9](?:[A-Za-z0-9-]{0,61}[A-Za-z0-9])?)(?:\\.(?:[A-Za-z0-9](?:[A-Za-z0-9-]{0,61}[A-Za-z0-9])?))+$",
                    "minLength": 0,
                    "maxLength": 255
                },
                "ClusterApiServerCaCertificateChain": {
                    "type": "string",
                    "maxLength": 2097152,
                    "minLength": 1
                },
                "ClusterName": {
                    "type": "string",
                    "pattern": "^[A-Za-z0-9][A-Za-z0-9-]{0,61}[A-Za-z0-9]$"
                },
                "RoleArn": {
                    "type": "string",
                    "relationshipRef": {
                        "typeName": "AWS::IAM::Role",
                        "propertyPath": "/properties/Arn"
                    }
                }
            },
            "required": [
                "EndpointsResourceName",
                "EndpointsResourceNamespace",
                "ClusterApiServerEndpointUri",
                "ClusterApiServerCaCertificateChain",
                "ClusterName",
                "RoleArn"
            ]
        },
        "ManagedEndpointConfiguration": {
            "type": "object",
            "oneOf": [
                {
                    "properties": {
                        "AutoScalingGroupsConfiguration": {
                            "$ref": "#/definitions/AutoScalingGroupsConfiguration"
                        }
                    },
                    "required": ["AutoScalingGroupsConfiguration"],
                    "additionalProperties": false
                },
                {
                    "properties": {
                        "TargetGroupsConfiguration": {
                            "$ref": "#/definitions/TargetGroupsConfiguration"
                        }
                    },
                    "required": ["TargetGroupsConfiguration"],
                    "additionalProperties": false
                },
                {
                    "properties": {
                        "EksEndpointsConfiguration": {
                            "$ref": "#/definitions/EksEndpointsConfiguration"
                        }
                    },
                    "required": ["EksEndpointsConfiguration"],
                    "additionalProperties": false
                }
            ]
        },
        "Tag": {
            "description": "A key-value pair to associate with a resource.",
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "Key": {
                    "type": "string",
                    "description": "The key name of the tag. You can specify a value that is 1 to 128 Unicode characters in length and cannot be prefixed with aws:. You can use any of the following characters: the set of Unicode letters, digits, whitespace, _, ., /, =, +, and -. ",
                    "minLength": 1,
                    "maxLength": 128
                },
                "Value": {
                    "type": "string",
                    "description": "The value for the tag. You can specify a value that is 0 to 256 Unicode characters in length and cannot be prefixed with aws:. You can use any of the following characters: the set of Unicode letters, digits, whitespace, _, ., /, =, +, and -. ",
                    "minLength": 0,
                    "maxLength": 256
                }
            },
            "required": [
                "Key"
            ]
        },
        "Tags": {
            "type": "array",
            "insertionOrder": false,
            "uniqueItems": true,
            "items": {
                "$ref": "#/definitions/Tag"
            },
            "minItems": 0,
            "maxItems": 50,
            "description": "Tags to assign to the Responder RtbApp."
        }
    },
    "properties": {
        "Tags": {
            "$ref": "#/definitions/Tags"
        },
        "AppName": {
            "$ref": "#/definitions/AppName"
        },
        "Description": {
            "$ref": "#/definitions/Description"
        },
        "VpcId": {
            "$ref": "#/definitions/VpcId"
        },
        "SubnetIds": {
            "$ref": "#/definitions/SubnetIds"
        },
        "SecurityGroupIds": {
            "$ref": "#/definitions/SecurityGroupIds"
        },
        "ClientToken": {
            "$ref": "#/definitions/ClientToken"
        },
        "RtbAppId": {
            "$ref": "#/definitions/RtbAppId"
        },
        "Status": {
            "$ref": "#/definitions/Status"
        },
        "CreatedTimestamp": {
            "$ref": "#/definitions/CreatedTimestamp"
        },
        "UpdatedTimestamp": {
            "$ref": "#/definitions/UpdatedTimestamp"
        },
        "Arn": {
            "$ref": "#/definitions/Arn"
        },
        "Port": {
            "$ref": "#/definitions/Port"
        },
        "Protocol": {
            "$ref": "#/definitions/Protocol"
        },
        "DnsName": {
            "$ref": "#/definitions/DnsName"
        },
        "CaCertificateChain": {
            "$ref": "#/definitions/CaCertificateChain"
        },
        "ManagedEndpointConfiguration": {
            "$ref": "#/definitions/ManagedEndpointConfiguration"
        }
    },
    "required": [
        "AppName",
        "VpcId",
        "SubnetIds",
        "SecurityGroupIds",
        "Port",
        "Protocol",
        "DnsName"
    ],
    "readOnlyProperties": [
        "/properties/RtbAppId",
        "/properties/Arn",
        "/properties/Status",
        "/properties/CreatedTimestamp",
        "/properties/UpdatedTimestamp"
    ],
    "conditionalCreateOnlyProperties": [
        "/properties/AppName",
        "/properties/Description",
        "/properties/VpcId",
        "/properties/SubnetIds",
        "/properties/SecurityGroupIds",
        "/properties/ClientToken",
        "/properties/Port",
        "/properties/Protocol",
        "/properties/DnsName",
