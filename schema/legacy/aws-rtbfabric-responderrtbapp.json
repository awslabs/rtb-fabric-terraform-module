    "tagging": {
        "taggable": true,
        "tagOnCreate": true,
        "tagUpdatable": true,
        "cloudFormationSystemTags": true,
        "tagProperty": "/properties/Tags",
        "permissions": [
            "mpofxdevmu:TagResource",
            "mpofxdevmu:UntagResource",
            "mpofxdevmu:ListTagsForResource"
        ]
    },
    "additionalProperties": false
}